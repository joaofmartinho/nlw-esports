{
  "root": true,
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier",
    "eslint-config-prettier"
  ],
  "env": {
    "es2021": true,
    "node": true
  },
  "rules": {
    "no-console": "error"
  }
}
